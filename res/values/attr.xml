<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="NumberView">
        <attr name="src" format="reference" />
        <attr name="number" format="string" />
        <attr name="textSize" format="integer" />
        <attr name="numberColor" format="color" />
    </declare-styleable>
    
    <!-- 自定义头像 -->
    <declare-styleable name="roundedimageview">
        <attr name="border_thickness" format="dimension" />
        <attr name="border_inside_color" format="color" />
        <attr name="border_outside_color" format="color"></attr>
    </declare-styleable>
    
    <!-- 图片指示器 -->
    <declare-styleable name="CustomIndicator">
	     <attr name="count" format="integer" />
	     <attr name="width" format="dimension" />
	     <attr name="height" format="dimension" />
	     <attr name="margin" format="dimension" />
	     <attr name="normal_icon" format="reference" />
	     <attr name="selected_icon" format="reference" />
	 </declare-styleable>

</resources>